{
  "name": "periop-calculators",
  "version": "1.3.0",
  "description": "Evidence-based perioperative risk assessment calculators for healthcare professionals",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
